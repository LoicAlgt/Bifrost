CREATE DATABASE loic;
USE loic;
CREATE TABLE password (sel_1 VARCHAR(500), sel_2 VARCHAR(500), sel_gcm VARCHAR(500),clefs VARCHAR(500),login VARCHAR(500),password VARCHAR(500));
CREATE TABLE credential (nom VARCHAR(50), value VARCHAR(50));
CREATE USER 'lolo'@'localhost' IDENTIFIED BY 'lolo';
GRANT ALL PRIVILEGES ON loic.* TO 'lolo'@'localhost';
